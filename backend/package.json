{
  "name": "build-arsenal-backend",
  "module": "server.ts",
  "type": "module",
  "version": "1.4.0",
  "description": "🏆 A+ Grade Enterprise Backend - Build Configuration Arsenal API v2.0 - Cloud-native build configuration management with production monitoring and enterprise security",
  "scripts": {
    "dev": "bun --watch server.ts",
    "start": "bun server.ts",
    "migrate": "bun run database/migrate.ts",
    "setup:aws": "bun run cloud/aws-setup.ts",
    "build": "bun build server.ts --outdir ./dist --target node",
    "test": "bun test",
    "lint": "bunx eslint . --ext .ts",
    "deploy": "bash scripts/production-deploy.sh",
    "health": "curl -s http://localhost:3001/health | jq .",
    "logs": "tail -f production.log",
    "backup": "bash scripts/backup.sh",
    "rollback": "bash scripts/rollback.sh"
  },
  "dependencies": {
    "hono": "^3.12.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "eslint": "^8.57.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2"
  },
  "engines": {
    "bun": ">=1.3.0",
    "node": ">=18.0.0"
  },
  "keywords": [
    "bun",
    "build",
    "bundler",
    "configuration",
    "api",
    "backend",
    "cloud-native"
  ],
  "author": "Bun Community",
  "license": "MIT"
}
